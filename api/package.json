{
  "name": "api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --onSuccess \"pnpm run start\"",
    "dev:debug": "tsup --watch --onSuccess \"DEBUG=loopback pnpm run start\"",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "format:ci": "prettier --check \"./**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "migrate": "node ./dist/migrate",
    "migrateAuth": "node ./server/scripts/createAuth",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "seed": "node ./server/scripts/seed",
    "start": "node -r source-map-support/register ."
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "dependencies": {
    "@loopback/boot": "^6.1.5",
    "@loopback/booter-lb3app": "^5.1.5",
    "@loopback/core": "^5.1.5",
    "@loopback/repository": "^6.1.5",
    "@loopback/rest": "^13.1.5",
    "@loopback/rest-explorer": "^6.1.5",
    "@loopback/service-proxy": "^6.1.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "lodash": "^4.17.10",
    "loopback": "^3.25.1",
    "loopback-boot": "^3.3.0",
    "loopback-component-explorer": "^6.0.0",
    "loopback-connector-mysql": "^7.0.6",
    "moment": "^2.24.0",
    "mysql2": "^3.9.0",
    "serve-favicon": "^2.0.1",
    "tiny-invariant": "^1.3.1",
    "tslib": "^2.0.0",
    "tsup": "^8.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.5",
    "@loopback/eslint-config": "^14.0.5",
    "@loopback/testlab": "^6.1.5",
    "@types/express": "^4.17.21",
    "@types/node": "^16.18.71",
    "async": "^2.6.1",
    "babel-eslint": "^8.2.5",
    "chalk": "^2.4.1",
    "eslint": "^8.51.0",
    "jest": "^23.1.0",
    "jest-preset-loopback": "^1.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^3.0.2",
    "nsp": "^2.1.0",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^3.1.0",
    "typescript": "~5.2.2"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "api"
}
